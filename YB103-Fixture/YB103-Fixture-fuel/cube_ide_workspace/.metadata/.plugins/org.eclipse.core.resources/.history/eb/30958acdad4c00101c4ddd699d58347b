            }
            break;
        case IHU_4:
            if(item < IHU_4_ITEM_MAX)
            {
                res = g_IHU_4[item];
            }
            break;
        case IHU_5:
            if(item < IHU_5_ITEM_MAX)
            {
                res = g_IHU_5[item];
            }
            break;
        case GW_BD_29:
            if(item < GW_BD_29_ITEM_MAX)
            {
                res = g_GW_BD_29[item];
            }
            break;
        case IHU_2:
            if(item < IHU_2_ITEM_MAX)
            {
                res = g_IHU_2[item];
            }
            break;
        case IHU_7:
            if(item < IHU_7_ITEM_MAX)
            {
                res = g_IHU_7[item];
            }
            break;
        case IHU_8:
            if(item < IHU_8_ITEM_MAX)
