                res = g_TBOX_1[item];
            }
            break;
        case TBOX_2:
            if(item < TBOX_2_ITEM_MAX)
            {
                res = g_TBOX_2[item];
            }
            break;
        case GW_PC_1:
            if(item < GW_PC_1_ITEM_MAX)
            {
                res = g_GW_PC_1[item];
            }
            break;
        case GW_PC_6:
            if(item < GW_PC_6_ITEM_MAX)
            {
                res = g_GW_PC_6[item];
            }
            break;
        case ESP_2:
            if(item < ESP_2_ITEM_MAX)
            {
                res = g_ESP_2[item];
            }
            break;
        case BCM_3:
            if(item < BCM_3_ITEM_MAX)
            {
                res = g_BCM_3[item];
            }
            break;
        case GW_BD_9:
            if(item < GW_BD_9_ITEM_MAX)
            {
                res = g_GW_BD_9[item];
            }
            break;
        case GW_BD_24:
            if(item < GW_BD_24_ITEM_MAX)
            {
                res = g_GW_BD_24[item];
            }
            break;
        default:
            break;
    }
    return res;
}

